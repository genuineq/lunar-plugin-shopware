{% block sw_order_list_grid_columns %}
    {% parent %}

    {% block lunar_payment_column_transaction %}
        <template #column-lunarPaymentTransactionId="{ item }">
            <b v-if="item.transactions && item.transactions.last() && item.transactions.last().customFields && item.transactions.last().customFields.lunar_payment_is_transaction">
                {{ item.transactions.last().id }}
            </b>
        </template>
    {% endblock %}
{% endblock %}
